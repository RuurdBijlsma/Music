{
    "name": "ruurd-music",
    "version": "6.0.0",
    "description": "Streaming music player",
    "keywords": [
        "Music",
        "Streaming",
        "Spotify"
    ],
    "private": true,
    "author": {
        "email": "ruurd@bijlsma.dev",
        "name": "Ruurd Bijlsma",
        "url": "ruurd.dev"
    },
    "repository": "https://github.com/ruurdbijlsma/ruurd-music",
    "main": "dist-electron/main/index.js",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build && electron-builder",
        "preview": "vite preview",
        "format": "prettier --write src/"
    },
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
    },
    "dependencies": {
        "anzip": "^0.2.0",
        "color-extr-thief": "^2.3.4",
        "events": "^3.3.0",
        "express": "^4.18.2",
        "ffbinaries": "github:ruurdbijlsma/ffbinaries-node.git#7c2b7e7958c70b1ddb6911ede6336cb294a86c74",
        "filenamify": "^6.0.0",
        "idb": "^7.1.1",
        "pinia": "^2.1.7",
        "replace-special-characters": "^1.2.7",
        "spotify-web-api-js": "^1.5.2",
        "vue": "^3.3.4",
        "vue-router": "^4.2.5",
        "vuetify": "^3.3.21",
        "yt-dlp-wrap": "^2.3.12"
    },
    "devDependencies": {
        "@mdi/font": "^7.1.96",
        "@types/contrast-color": "^1.0.0",
        "@types/events": "^3.0.0",
        "@types/express": "^4.17.17",
        "@types/ffbinaries": "^1.1.2",
        "@types/node": "^18.14.2",
        "@vitejs/plugin-vue": "^4.4.0",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/tsconfig": "^0.4.0",
        "electron": "^27.0.0",
        "electron-builder": "^24.6.4",
        "prettier": "^3.0.3",
        "sass": "^1.69.3",
        "typescript": "^5.2.2",
        "vite": "^4.4.11",
        "vite-plugin-vuetify": "^1.0.2",
        "vite-plugin-electron": "^0.14.1",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vite-plugin-top-level-await": "^1.3.1",
        "vue-tsc": "^1.8.19"
    }
}
